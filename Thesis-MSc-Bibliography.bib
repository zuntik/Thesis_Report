	Keywords = {Internet;peer-to-peer computing;video streaming;Internet application;Internet connectivity;low-complexity content adaptation;peer-to-peer streaming;scalable video;server-client approach;Bandwidth;Internet;Peer to peer computing;Protocols;Static VAr compensators;Streaming media;Technology forecasting},
	Number = {3},
